@charset "UTF-8";nav{padding:10px;width:80%;margin:auto;display:block;color:var(--primary)}nav ul{list-style:none;margin-right:0;padding:0}nav ul a{color:var(--primary);text-decoration:none;font-weight:700}nav ul a:hover{text-decoration:underline}nav ul li{display:inline}.project-footer-custom .my-custom-bottom-nav{position:static!important;top:auto!important;display:block!important;background:0 0!important}.project-footer-custom .my-custom-bottom-nav .nav-links{display:flex;justify-content:space-between;align-items:center;width:100%}.project-footer-custom .my-custom-bottom-nav .nav-links .nav-prev,.project-footer-custom .my-custom-bottom-nav .nav-links .nav-next{display:inline-flex;align-items:center;padding:12px 20px;background-color:#fff;border:1px solid #e1e4e8;border-radius:8px;color:#24292e;text-decoration:none;font-size:.95rem;font-weight:500;max-width:45%;box-shadow:0 1px 3px rgba(0,0,0,4%)}.project-footer-custom .my-custom-bottom-nav .nav-links .nav-prev:hover,.project-footer-custom .my-custom-bottom-nav .nav-links .nav-next:hover{border-color:#0366d6;color:#0366d6;background-color:#f1f8ff;transform:translateY(-2px)}.project-footer-custom .my-custom-bottom-nav .nav-links .nav-prev .nav-title,.project-footer-custom .my-custom-bottom-nav .nav-links .nav-next .nav-title{overflow:hidden;text-overflow:ellipsis;white-space:nowrap;margin:0 8px}.project-footer-custom .my-custom-bottom-nav .nav-links .nav-next{text-align:right}.nav-text{display:inline-block;max-width:100%;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;vertical-align:bottom}@media(max-width:600px){.nav-text{display:none}}.header-spacer{height:10vmin;min-height:80px}.header-spacer.home-spacer{height:20vmin;min-height:120px}@media screen and (max-width:768px){.header-spacer{height:100px!important}.header-spacer.home-spacer{height:140px!important}}